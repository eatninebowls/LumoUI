{
  "name": "@eatninebowls/lumoui",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:play": "vite --config ./playground/vite.play.config.ts",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:deploy": "gh-pages -d docs/.vitepress/dist",
    "docs:preview": "vitepress preview docs"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./styles/index.css": "./dist/lumoui.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eatninebowls/LumoUI.git"
  },
  "homepage": "https://eatninebowls.github.io/LumoUI/",
  "bugs": {
    "url": "https://github.com/eatninebowls/LumoUI/issues"
  },
  "dependencies": {
    "@eatninebowls/lumoui": "^0.0.0",
    "@tailwindcss/vite": "^4.1.13",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitepress-demo-preview/component": "^2.5.2",
    "@vitepress-demo-preview/plugin": "^1.4.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.9.2",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^1.6.4",
    "vitepress-demo-plugin": "^1.5.0",
    "vue-tsc": "^3.0.5"
  }
}
